{
  "scripts": {
    "build": "npx tsc",
    "watch": "nodemon --legacy-watch --watch './src/**' --ext 'ts,json' --exec 'npm run dev'",
    "dev": "npx tsc && cp -f .env ./src/ && ts-node ./src/index.ts",
    "prod": "npx tsc && cp -f .env ./dist/ && node ./dist/index.js"
  },
  "dependencies": {
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "request": "^2.88.2",
    "request-promise-native": "^1.0.9"
  },
  "devDependencies": {
    "@types/express": "^4.17.12",
    "@types/node": "^15.12.2",
    "@types/request": "^2.48.5",
    "@types/request-promise-native": "^1.0.17",
    "nodemon": "^2.0.7",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  }
}
